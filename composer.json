    "homepage": "https://treecode.ru",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Abeliani\\TranslatedSlugHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abeliani\\TranslatedSlugHelper\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Anatolii Belianin",
            "email": "belianianatoli@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "abeliani/string-translator": "^1.0",
        "abeliani/slug-helper": "^1.0",
        "guzzlehttp/guzzle": "^7.8.0"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^2.0"
    }
}
